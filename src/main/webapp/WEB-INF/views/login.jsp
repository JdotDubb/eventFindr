            <li class="{{#if sponsored}}sponsored{{/if}}" data-type="{{this.type}}" {{#if this.seid}}data-seid="{{this.seid}}"{{/if}} {{#if this.spid}}data-spid="{{this.spid}}" data-permalink="{{this.permalink}}"{{/if}} {{#if this.svid}}data-svid="{{this.svid}}"{{/if}} {{#if this.smid}}data-smid="{{this.smid}}"{{/if}} {{#if this.name_squish}}data-name-squish="{{this.name_squish}}"{{/if}} {{#if this.location_id}}data-location-id="{{this.location_id}}" data-location-type="{{this.location_type}}"{{/if}} {{#if this.cta_link}}data-cta-link="{{this.cta_link}}"{{/if}} data-new-tab="{{this.open_in_new_tab}}" {{#if sponsored}} data-ga-label="{{this.tracking}}"{{/if}}>
            {{truncate this.name 50}}
            
            <p>{{itemDescription this}}</p>
            {{#if sponsored}}
